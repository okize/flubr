{
  "name": "Flubr",
  "description": "pass/fail images for chatrooms, moderated by your team members",
  "website": "http://okize.github.io/flubr/",
  "repository": "https://github.com/okize/flubr",
  "logo": "https://raw.githubusercontent.com/okize/flubr/gh-pages/logo.png",
  "keywords": [
    "pass",
    "fail",
    "images",
    "chat",
    "hubot",
    "flubr"
  ],
  "success_url": "/",
  "addons": [
    "mongohq"
  ],
  "env": {
    "BUILDPACK_URL": "https://github.com/heroku/heroku-buildpack-nodejs",
    "NODE_ENV": "production",
    "SECRET_TOKEN": {
      "description": "A secret key for verifying the integrity of signed cookies.",
      "generator": "secret"
    },
    "IMGUR_CLIENTID": "",
    "TWITTER_CONSUMER_KEY": "",
    "TWITTER_CONSUMER_SECRET": "",
    "TWITTER_ACCESS_TOKEN": "",
    "TWITTER_ACCESS_TOKEN_SECRET": ""
  }
}
